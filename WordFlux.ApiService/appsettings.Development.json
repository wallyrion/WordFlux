{
  "OtelEndpointSeq": "http://localhost:5343/ingest/otlp/v1/traces",
  "OtelHeadersSeq": "",
  "OTEL_SERVICE_NAME": "Wordflux.Local",
  "OtelEndpointAspireDashboard": "http://localhost:5569",
  "OtelHeadersAspireDashboard": "x-otlp-api-key=dev",
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://localhost:5343/",
          "apiKey": ""
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "WordFlux.Api"
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "UseAzureKeyVault": "false"
}
