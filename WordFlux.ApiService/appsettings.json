{
  "OtelEndpointSeq": "http://172.191.101.172:80/ingest/otlp/v1/traces",
  "OtelHeadersSeq": "",
  "OTEL_SERVICE_NAME": "Wordflux.Api",
  "OtelEndpointAspireDashboard": "",
  "OtelHeadersAspireDashboard": "",
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Information",
      "Microsoft.AspNetCore.Diagnostics.ExceptionHandlerMiddleware": "None",
      "Microsoft.AspNetCore.HttpLogging.HttpLoggingMiddleware": "Information",
      "Microsoft.AspNetCore.Cors.Infrastructure.CorsService" :"Information",
      "Microsoft.AspNetCore.Http.Result.OkObjectResult" :"Information",
      "Microsoft.AspNetCore.Hosting.Diagnostics": "Information"
    }
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Seq"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "Seq",
        "Args": {
          "serverUrl": "http://172.191.101.172:80/",
          "apiKey": ""
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "WordFlux.Api"
    }
  },

  "AllowedHosts": "*",
  "UseAzureKeyVault": "true",
  "OpenAIKey": "",
  "AzureAiTranslatorKey": "",
  "DeeplAuthKey": "",
  "BingSearchApiKey": "",
  "UnsplashApiKey": "",
  "Jwt": {
    "Key": "YourVeryLongSecretKeyHere1234567890",
    "Issuer": "YourIssuer",
    "Audience": "YourAudience"
  },
  "CurrentImageTag": "latest"
}
